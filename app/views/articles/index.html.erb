<div class="container" style="width: 800px;">
  <ul class="list-unstyled my-5">
    <% @articles.each do |article| %>
      <li>
        <h3><%= link_to article.title, article_path(article.id), style: "text-decoration: none;" %></h3>
        <p><%= article.content%></p>
      </li>
    <% end %>
  </ul>
</div>
